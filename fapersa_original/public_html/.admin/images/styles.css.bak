/*
drag-box
*/
#boxes {
	font-family: Arial, sans-serif;
	list-style-type: none;
	margin: 0px;
	padding: 0px;
	width: 830px;
}

#boxes li {
	cursor: move;
	position: relative;
	float: left;
}

/*
POPUP
*/

.popupfadewarning .popup{ background-color: #b0be96; }
.popupfade{
	position: absolute;
	background-color: #333333;
}
div.popup h1 {
	font-size: 16px;
	margin: 0;
	letter-spacing: -1px;
}
div.popup{
    position: absolute;
	background-color: #EEEEEE;
	padding: 10px; 
	width: 400px;
	text-align: left;
}
div.popup .popupclose{
	display: none;
    float: right;
    margin: -35px -10px 0 0;
    cursor: pointer;
    color: #bbbbbb;
}
div.popup .popupcontainer{
	background-color: #F5F5F5;
	padding: 5px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #333333;
	height: 190px;
}
div.popup .popupmessage{
	background-color: #FFFFFF;
	padding: 10px;
	width: 280px;
	height: 170px;
	float: left;
}
div.popup .jqibuttons{
	text-align: center;
	padding: 5px 0 0 0;
}
div.popup button{
	background: #ffffff;
	border: solid #777777 1px;
	font-size: 12px;
	padding: 3px 10px 3px 10px;
	margin: 5px 5px 5px 10px;
	width: 75px;
}
div.popup button:hover{
	border: solid #aaaaaa 1px;
}
