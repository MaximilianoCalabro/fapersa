body {
	 margin-left: 0px;
	 font-family: Verdana, Tahoma, Arial;
	 font-size: 8pt;
}

label {
	cursor: pointer;
}

.padding {
	padding: 8px 4px 8px 4px;
}

.password-strength-bar {
	padding: 3px;
	height: 12px;
	width: 100px;
	text-align: center;
	font-weight: bold;
	border: 1px solid #666;
	color: #FFFFFF;
	display: block;
}

.hidden {
	display: none;
}

.header {
	font-family: Tahoma, Arial, Verdana;
	font-size: 11px;
	color: #000;
}

.fields_stylish {
	background: url(../images/bg-fields.gif) repeat-x;
	background-color: #FFFFFF;
	font-family: Tahoma, Arial, Verdana;
	font-size: 12px;
	color: #333333;
	border: 1px solid #C1D5D5;
	padding: 3px;
}

.button_stylish {
	background: url(../images/bg-button.gif) repeat-x;
	background-color: #FFFFFF;
	font-family: "Century Gothic", Arial, Helvetica, sans-serif;
	font-weight: bold;
	font-size: 14px;
	text-transform: uppercase;
	color: #333333;
	border: 1px solid #C1D5D5;
	letter-spacing: -1px;
	padding: 6px;
	text-decoration: none;
}

.fields {
	border: 1px solid #666666;
	font-family: Tahoma, Verdana;
	font-size: 11px;
	scrollbar-base-color:		#666666;
	scrollbar-arrow-color:      #FFFFFF;
	padding: 4px;
}

.fields_error {
	border: 1px solid #666666;
	font-family: Tahoma, Verdana;
	font-size: 11px;
